import math
def contar_primos(numero):
    if numero <= 1:
        return False
    for i in range(2, int(numero**0.5) + 1):
        if numero % i == 0:
            return False
    return True

lista_primos = [11]
numero = int(input("Introduce un número inicial para comenzar a buscar primos: "))

